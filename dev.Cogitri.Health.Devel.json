{
  "app-id": "dev.Cogitri.Health.Devel",
  "runtime": "org.gnome.Platform",
  "runtime-version": "42",
  "sdk": "org.gnome.Sdk",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "command": "dev.Cogitri.Health.Devel",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--socket=wayland",
    "--device=dri"
  ],
  "tags": [
    "devel",
    "development",
    "nightly"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "/share/vala",
    "*.la",
    "*.a",
    "/lib/girepository-1.0",
    "/share/gir-1.0"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "build-args": [
      "--share=network"
    ],
    "env": {
      "RUST_BACKTRACE": "1"
    }
  },
  "modules": [
    {
      "name": "blueprintcompiler",
      "buildsystem": "meson",
      "builddir": true,
      "sources": [
        {
          "type": "git",
          "url": "https://gitlab.gnome.org/jwestman/blueprint-compiler",
          "commit": "93f2a27e35905420e70bfdb5c2f04d6af43b357d"
        }
      ]
    },
    {
      "name": "tracker",
      "buildsystem": "meson",
      "builddir": true,
      "config-opts": [
        "-Dman=false",
        "-Dsystemd_user_services=false"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://download.gnome.org/sources/tracker/3.3/tracker-3.3.0.tar.xz",
          "sha512": "0a55a80b44c2267f149c4c83ecbe42ca27a0645bd4fc206984999520f6bed3b7c63c71f81450f40ce2896223ce17326add20e7f39a71c821d754f30b8ce157bc"
        }
      ]
    },
    {
      "name": "health",
      "builddir": true,
      "buildsystem": "meson",
      "cleanup": [
        "/usr/"
      ],
      "config-opts": [
        "-Dprofile=development"
      ],
      "sources": [
        {
          "type": "dir",
          "path": "."
        }
      ]
    }
  ]
}
