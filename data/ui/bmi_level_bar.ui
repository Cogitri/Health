<interface domain="dev.Cogitri.Health">
  <requires lib="gtk" version="4.0"/>
  <template class="HealthBMILevelBar" parent="GtkWidget">
    <child>
      <object class="GtkLabel" id="bmi_label">
        <property name="use-markup">1</property>
      </object>
    </child>
    <child>
      <object class="GtkLevelBar" id="level_bar"/>
    </child>
  </template>
</interface>
